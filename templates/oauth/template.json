{
  "name": "oauth",
  "description": "OAuth authentication with GitHub and Google integration",
  "variables": [
    {
      "name": "PROJECT_NAME",
      "prompt": "Project name",
      "type": "string",
      "default": "MyOAuthApp"
    },
    {
      "name": "GITHUB_CLIENT_ID",
      "prompt": "GitHub OAuth Client ID",
      "type": "string",
      "secret": true,
      "default": "your_github_client_id"
    },
    {
      "name": "GOOGLE_CLIENT_ID",
      "prompt": "Google OAuth Client ID",
      "type": "string",
      "secret": true,
      "default": "your_google_client_id"
    },
    {
      "name": "JWT_SECRET",
      "prompt": "JWT Secret Key",
      "type": "string",
      "secret": true,
      "default": "your_jwt_secret_key_change_in_production"
    },
    {
      "name": "ENABLE_GITHUB",
      "prompt": "Enable GitHub OAuth?",
      "type": "boolean",
      "default": true
    },
    {
      "name": "ENABLE_GOOGLE",
      "prompt": "Enable Google OAuth?",
      "type": "boolean",
      "default": true
    }
  ]
}